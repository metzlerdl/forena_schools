<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE root [
<!ENTITY nbsp "&#160;">
]>
<html xmlns:frx="urn:FrxReports">
<head>
<frx:options hidden="1" />
<frx:fields>
	<frx:field id="bldg.name"
		link="reports/BuildingDashboard?bldg_id={bldg_id}" />
	<frx:field id="grade_name"
		link="{bldg.building_dashboard}?bldg_id={bldg_id}&amp;grade_level={parm.grade_level}" />
	<frx:field id="last_name" link="student/{person_id}" />
	<frx:field id="export"
		link="reports/group.TestResults.csv?test_id={parm.test_id}&amp;group_id={parm.group_id}&amp;grade_level={parm.grade_level}&amp;school_year={parm.school_year}&amp;seq={parm.seq}">Export to .csv</frx:field>
</frx:fields>
<title>Test Results</title>
</head>
<body>
	<div id="schedule" frx:block="ims/tests/schedule" frx:foreach="*" >
 </div> 
  <div frx:block="ims/group/details" id="group" frx:foreach="*">
   <h1 frx:renderer="frxTitle">{group.name}, {year_label} {test_name} - {sched_label}</h1>
   <h3>{first_name} {last_name}</h3>
 </div>
<h3>Scores</h3>
<div class="pedagoggle-scores"
				frx:block="ims/group/group_test_scores">
				<table frx:renderer="FrxPedagoggleScores" frx:profile="schedule">
					<thead>
						<tr>
							<th>Name</th>
						</tr>
					</thead>
					<tbody>
						<tr>
							<td>{last_name}, {first_name}</td>
						</tr>
					</tbody>
				</table>
			</div>
 <h3>Statistics</h3>
 <div
				frx:src="reports/group.TestStatsGraph.svg?group_id={parm.group_id}&amp;grade_level={parm.grade_level}&amp;test_id={parm.test_id}&amp;school_year={parm.school_year}&amp;seq={parm.seq}"
				frx:renderer="FrxInclude" frx:height="600" frx:width="800" />
 <div id="schedule-stats" frx:block="ims/group/group_test_statistics">
				<p>
					<strong>Performance Distribution:</strong> The above graph
					demonstrates the percentage of students tested performing at each
					level.
				</p>
				<table class="table table-bordered">
					<thead>
						<tr>
							<th>measure</th>
							<th>building average</th>
							<th>district average</th>
							<th>struggling</th>
							<th>below</th>
							<th>met</th>
							<th>exceeded</th>
							<th>percent met standard</th>
						</tr>
					</thead>
					<tbody>
						<tr frx:foreach="*">
							<td>{name}</td>
							<td>{avg_score}</td>
							<td>{b_score}</td>
							<td>{l1_percent}</td>
							<td>{l2_percent}</td>
							<td>{l3_percent}</td>
							<td>{l4_percent}</td>
							<td>{percent_met}</td>
						</tr>
					</tbody>
				</table>
</div>
<div id="footer-block">
		<ul class="nav hnav">
			<li>{export}</li>
		</ul>
</div>
</body>
</html>