<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE root [
<!ENTITY nbsp "&#160;">
]>
<html xmlns:frx="urn:FrxReports">
<head>
<frx:options hidden="1"/> 
<frx:fields> 
   <frx:field id="last_name" link="student/{person_id}"/> 
   <frx:field id="export" link="reports/group.GroupProfile.csv?profile_id={parm.profile_id}&amp;group_id={parm.group_id}">Export to .csv</frx:field> 
</frx:fields>
<title>Group View</title>
</head>
<body>
  <div id="profile-block" frx:block="ims/group/group_profile">
    <div frx:foreach="*" id="profile">
      <h3>{name}, {year_label}</h3>
      <div class="pedagoggle-scores" frx:block="ims/group/group_profile_scores">
      <table class="table" frx:renderer="FrxPedagoggleScores" frx:profile="profile">
        <thead>
          <tr>
            <th>Name</th>
            <th style="display: none;">SIS ID</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <td>{last_name}, {first_name}</td>
            <td style="display:none;">{sis_id}</td>
          </tr>
        </tbody>
      </table>
      </div>
    </div>
  </div>
  <div id="footer-block" >
    <ul class="nav hnav">
     <li>{export}</li>
    </ul>
  </div>
</body>
</html>