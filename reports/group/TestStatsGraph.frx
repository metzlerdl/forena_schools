<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE root [
<!ENTITY nbsp "&#160;">
]>
<html xmlns:frx="urn:FrxReports">
<head>
<frx:fields>

</frx:fields>
<title>Test Statistics</title>
</head>
<body>
  <div class="grid_12" >
    <div id="schedule" frx:block="ims/tests/schedule" frx:foreach="*">
      <h4>{sched_label} {test_name} {year_label}</h4>
  </div>
  <div id="schedule-stats" frx:block="ims/group/group_test_statistics">
			<svg id="test-chart" frx:renderer="FrxSVGGraph"
				frx:link="group.MeasureStatistics?group_id={parm.group_id}&amp;measure_id={measure_id}&amp;school_year={parm.school_year}&amp;seq={parm.seq}"
				frx:type="stackedbargraph" frx:xpath="*" frx:height="600"
				frx:width="800"
				frx:options="series[]={l1_percent}&amp;series[]={l2_percent}&amp;series[]={l3_percent}&amp;series[]={l4_percent}&amp;label={abbrev}&amp;colors[]=#E3B5B5&amp;colors[]=#E3E288&amp;colors[]=#BBE3B5&amp;colors[]=#B5C0E3&amp;axis_max_v=100.0&amp;grid_division_v=10">
	  </svg>
			<table>
	    <thead>
	      <tr><th>measure</th><th>building average</th><th>district average</th><th>struggling</th><th>below</th><th>met</th><th>exceeded</th><th>percent met standard</th></tr>
	    </thead>
	    <tbody>
	      <tr frx:foreach="*">
	        <td>{name}</td><td>{b_score}</td><td>{d_score}</td><td>{l1_percent}</td><td>{l2_percent}</td><td>{l3_percent}</td><td>{l4_percent}</td><td>{b_percent_met}</td>
	      </tr>
	    </tbody>
	  </table>
  </div>
  </div>
</body>
</html>