<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE root [
<!ENTITY nbsp "&#160;">
]>
<html xmlns:frx="urn:FrxReports">
<head>
<frx:fields>
</frx:fields>
<title>Measure Statistics</title>
</head>
<body>
		<div id="test" frx:block="ims/tests/measure" frx:foreach="*">
			<h3>{test_name}: {name}</h3>
		</div>
		<div id="measure_stats" frx:block="ims/group/measure_score_stats">
			<svg id="measure-chart" frx:renderer="FrxSVGGraph"
				frx:type="groupedbargraph" frx:xpath="*" frx:height="{parm.height}"
				frx:width="{parm.width}"
				frx:options="series[]={avg_score}&amp;series[]={b_score}&amp;series[]={d_score}&amp;label={year_label}+{sched_label}&amp;colors[]=tan&amp;colors[]=olivedrab&amp;colors[]=olivedrab">
    </svg>
			<table>
				<thead>
					<tr>
						<th>measure</th>
						<th>score</th>
						<th>building average</th>
						<th>district average</th>
						<th>percent met standard</th>
					</tr>
				</thead>
				<tbody>
					<tr frx:foreach="*">
						<td>{year_label} {sched_label}</td>
						<td>{avg_score}</td>
						<td>{b_score}</td>
						<td>{d_score}</td>
						<td>{b_percent_met}</td>
					</tr>
				</tbody>
			</table>
		</div>
</body>
</html>