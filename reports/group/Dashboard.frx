<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE root [
<!ENTITY nbsp "&#160;">
]>
<html xmlns:frx="urn:FrxReports">
<head>
<frx:options hidden="1"/> 
<frx:fields> 
   <frx:field id="name" link="student/{person_id}"/> 
   <frx:field id="test_name" link="reports/group.TestResults?group_id={group_id}&amp;test_id={test_id}&amp;seq={seq}&amp;school_year={school_year}&amp;grade_level={grade_level}"/>
   <frx:field id="profile.name" link="reports/group.GroupProfile?group_id={parm.group_id}&amp;profile_id={profile_id}"/> 
   <frx:field id="entry.sched_label" link="flexpopup/TestEntry?group_id={group_id}&amp;test_id={test_id}&amp;seq={seq}" target="popup_test_entry_{group_id}"/>
   <frx:field id="edit_group" link="flexpopup/GroupEditor?group_id={parm.group_id}" target="popup_group_edit_{parm.group_id}">Edit Group</frx:field>  
</frx:fields>
<frx:menu path="group/:group_id/dashboard" title="View" type="default-local-task"/> 
<title>Group</title>
</head>
<body>
 <div frx:block="ims/group/details" id="group" frx:foreach="*" class="span12">
   <h1 frx:renderer="FrxTitle">{group.name}, {group.year_label}</h1>
   <h2>Teacher: {first_name} {last_name}</h2>
 </div>
 <p />
 <div class="span2">
    <div frx:block="ims/group/scheduled_tests" class="side-block">
      <h2>Test Entry</h2>
      <ul class="nav">
        <li id="entry" frx:foreach="*">{entry.sched_label}</li>
      </ul>
    </div>
    <div frx:block="ims/group/group_profiles" id="profiles" class="side-block">
       <h2>Profiles</h2>
       <ul class="nav">
         <li frx:foreach="*" id="profile">{profile.name}</li>
       </ul>
    </div>
  </div>
<div class="span1"> </div>
 <div class="span9">
 <div  frx:block="ims/group/testing_summary" id="test-query">
  <p>The Following is a list of tests available for students at {group.bldg_abbrev}.<br />Make a selection to view data available for that test.</p>
   <table class="table table-bordered">
     <thead>
       <tr>
       <th>Test Name</th>
       <th>Grade</th>
       <th>Schedule</th>
       <!--<th>coverage</th>-->
       </tr>
     </thead>
     <tbody>
       <tr frx:foreach="*" id="test">
         <td>{test_name}</td>
         <td>{year_label} {grade}</td>
         <td>{sched_label}</td>
         <!--<td>{coverage}</td>-->
       </tr>
     </tbody>
   </table>
 </div>
 <div  frx:block="ims/group/members">
   
   <table class="table table-bordered">
      <thead>
        <tr>
          <th>Students</th>
        </tr>
      </thead>
        <tbody>
          <tr frx:foreach="*">
            <td>{name}</td>
          </tr>
        </tbody>
      </table>
  </div>
  
 </div>
</body>
</html>