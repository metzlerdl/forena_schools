<?xml version="1.0"?>
<html xmlns:frx="urn:FrxReports">
<head>
<title>District Dashboard</title>
<frx:category>District</frx:category>
<frx:options hidden="1"/>
<frx:fields>
	<frx:field id="@title" link="{@path}" target="{@target}" />
	<frx:field id="name" link="reports/BuildingDashboard?bldg_id={bldg_id}" />
  <frx:field id="grade_level_name" link="reports/district.GradeDashboard?grade_level={grade_level}"/> 
</frx:fields>
<frx:parameters>
</frx:parameters>
<frx:docgen>
</frx:docgen>

</head>
<body>
	<div class="span2">
		<div frx:block="menus/district" class="side-block">
			<h2>District</h2>
			<ul class="nav">
				<li frx:foreach="*">{@title}
				</li>
			</ul>
		</div>

		<div frx:block="menus/admin" class="side-block">
			<h2>Admin</h2>
			<ul class="nav">
				<li frx:foreach="*">{@title}
				</li>
			</ul>
		</div>
	</div>
	<div class="span1"></div>
	<div frx:block="ims/buildings" id="forena-1" class="span9 nav">
		<h2>Schools</h2>
		<ul>
			<li frx:foreach="*" id="forena-2">{name}
			</li>
		</ul>
	</div>
  <div frx:block="ims/grade_levels" class="span3">
    <h2>District Grade Level Data</h2>
    <ul>
      <li frx:foreach="*">{grade_level_name}</li>
    </ul>
  </div>
</body>
</html>
