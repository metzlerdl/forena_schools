<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE root [
<!ENTITY nbsp "&#160;">
]>
<html xmlns:frx="urn:FrxReports">
<head>
<frx:options hidden="1"/> 
<frx:fields>
  <frx:field id="name" link="reports/GroupView.web?profile_id={profile_id}&amp;group_id={group_id}"/> 
   <frx:field id="last_name" link="student/{person_id}"/> 
   <frx:field id="export" link="reports/GroupView.csv?profile_id={parm.profile_id}&amp;group_id={parm.group_id}">Export to .csv</frx:field> 
</frx:fields>
<title>Group View</title>
</head>
<body>
 <div frx:block="ims/group/details" id="group" frx:foreach="*" class="grid_12">
   <h2>{group.name}, {group.year_label}</h2>
   <h4><small>{first_name} {last_name}</small></h4>
 </div>
  <div id="profile-block" frx:block="ims/group/group_profile">
    <div frx:foreach="*" id="profile">
      <h3>{name}</h3>
      <div class="pedagoggle-scores" frx:block="ims/group/group_profile_scores">
      <table frx:renderer="FrxPedagoggleScores" frx:profile="profile" class="table">
        <thead>
          <tr>
            <th>Name</th>
            <th style="display: none;">SIS id</th>
            <th style="display: none;">Building</th>
            <th style="display: none;">Grade</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <td>{last_name}, {first_name}</td>
            <td style="display; none;">{sis_id}</td>
            <td style="display: none;">{sis_code}</td>
            <td style="display: none;">{grade}</td>
          </tr>
        </tbody>
      </table>
      </div>
    </div>
  </div>
  <div id="footer-block" >
    <ul class="nav hnav">
     <li>{export}</li>
    </ul>
  </div>
</body>
</html>