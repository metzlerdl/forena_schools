<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE root [
<!ENTITY nbsp "&#160;">
]>
<html xmlns:frx="urn:FrxReports">
  <head>
    <title>Define School Years</title>
    <frx:category>Admin</frx:category>
    <frx:options/>
    <frx:fields>
      <frx:field id="name" link="forenaschools/year/edit">Add</frx:field>
      <frx:field id="year_label" link="forenaschools/year/edit/{school_year}"/> 
    </frx:fields>
    <frx:parameters><frx:parm id="school_year"/></frx:parameters>
    <frx:docgen/>
  </head>
  <body>

  <div id="school_years_block" class="FrxTable" frx:block="ims/school_years">
    <p>{add}</p>
    <table>
      <thead>
        <tr>
          <th>school_year</th>
          <th>year_label</th>
          <th>start_date</th>
          <th>end_date</th>
          <th>year_selected</th></tr></thead>
      <tbody>
        <tr id="school_years" frx:foreach="*">
          <td>{school_year}</td>
          <td>{year_label}</td>
          <td>{start_date}</td>
          <td>{end_date}</td>
          <td>{year_selected}</td></tr></tbody></table></div></body>
</html>
