<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE root [
<!ENTITY nbsp "&#160;">
]>
<html xmlns:frx="urn:FrxReports">
<head>
<title>Grade Level View</title>
<frx:category>District</frx:category>
<frx:options hidden="1" form="" />
<frx:parameters>
</frx:parameters>
<frx:fields>
  <frx:field id="student_search" class="btn btn-primary" link="schools/{bldg_id}/search">Student Search</frx:field>
  <frx:field id="profile_name"
    link="reports/district.GradeView?bldg_id={parm.bldg_id}&amp;grade_level={parm.grade_level}&amp;profile_id={profile_id}&amp;school_year={parm.school_year}" />
  <frx:field id="group_name" link="group/{group_id}" />
  <frx:field id="manage" link="flexpopup/GroupEditor?group_id={@group_id}" target="popupGroupEditor{@group_id}">manage</frx:field>
  <frx:field id="year_label"
    link="reports/district.GradeDashboard?grade_level={parm.grade_level}&amp;school_year={school_year}" />
  <frx:field id="grade_level_name"
    link="reports/district.GradeDashboard?grade_level={grade_level}&amp;school_year={parm.school_year}" />
  <frx:field id="test_name"
    link="reports/district.GradeTestView?grade_level={parm.grade_level}&amp;test_id={test_id}&amp;school_year={school_year}&amp;seq={seq}">
  </frx:field>
</frx:fields>
<frx:parameters />
<frx:docgen />
</head>
<body>

  <div class="nav span2 pull-left">

    <div frx:block="ims/profiles/by_grade" id="profiles" class="nav side-block">
      <h2>Profiles</h2>
      <ul class="nav">
        <li frx:foreach="*">{profile_name}</li>
      </ul>
    </div>

    <div frx:block="ims/grade_levels" class="nav side-block">
      <h2>Grade Level</h2>
      <ul class="nav">
        <li frx:foreach="*">{grade_level_name}</li>
      </ul>
    </div>

    <div id="school_years" frx:block="ims/school_years" class="side-block">
      <h2>Year</h2>
      <ul class="nav">
        <li frx:foreach="*" class="{year_selected}" id="forena-12">{year_label}</li>
      </ul>
    </div>
  </div>
  <div class="span9">
    <div id="bldg" frx:block="ims/district/grade" frx:foreach="*">
      <h2>{grade_name}, {bldg.year_label}</h2>
    </div>

    <div id="test_stats" frx:block="ims/district/test_stats">
      <h3>Test Statistics</h3>
      <p>The following table shows the most recent calculated statistics for all tests available for {bldg.name}. Select a
        test to view statistics available for that test.</p>
      <table class="table table-bordered">
        <thead>
          <tr>
            <th>Test Name</th>
            <th>Test Schedule</th>
          </tr>
        </thead>
        <tbody>
          <tr frx:foreach="*" id="schedule">
            <td>{test_name}</td>
            <td>{sched_label} {schedule.year_label}</td>
          </tr>
        </tbody>
      </table>
    </div>
    <div frx:block="ims/building/groups_for_grade_level" id="collections">
      <h3>Intervention Groups</h3>
      <table class="table table-bordered">
        <tbody>
          <tr frx:foreach="*" id="coll">
            <td>{group_name}</td>
            <td>{grade}</td>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
</body>
</html>
