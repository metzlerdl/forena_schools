<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE root [
<!ENTITY nbsp "&#160;">
]>
<html xmlns:frx="urn:FrxReports">
<head>
<title>Profile Graph Test</title>
<frx:category>Student</frx:category>
<frx:options />
<frx:fields />
<frx:parameters>
	<frx:parm id="profile_id" type="select" data_source="ims/student/profiles" data_field="profile_id" label_field="name"/> 
	<frx:parm id="school_year" type="hidden"></frx:parm>
</frx:parameters>
<frx:docgen />
</head>
<body>

	<div id="subject_performance"
		frx:block="ims/student/profile_performance">
		<svg frx:type="scatterplot" frx:renderer="FrxSVGGraph"
		  frx:series_1="{norm_score}"
		  frx:seriesx_1="{grade}"
		  frx:best_fit="straight"
      frx:label_x="Grade Level"
      frx:label_y='Normalized Score'
      
		  />
		<table>
		<tr frx:foreach="*">
		  <td><span frx:renderer="FrxXMLSource"/></td>
		</tr></table>
	</div>
</body>
</html>
