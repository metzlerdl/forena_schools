<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE root [
<!ENTITY nbsp "&#160;">
]>
<html xmlns:frx="urn:FrxReports">
<head>
<frx:options hidden="1"/>
<frx:fields>
  <frx:field id="bldg.name" link="reports/BuildingDashboard?bldg_id={bldg_id}"/>
  <frx:field id="grade_name" link="{bldg.building_dashboard}?bldg_id={bldg_id}&amp;grade_level={parm.grade_level}"/>
  <frx:field id="name" link="reports/GradeView.web?profile_id={profile_id}&amp;bldg_id={bldg_id}&amp;grade_level={grade_level}"/> 
  <frx:field id="last_name" link="student/{person_id}"/> 
  <frx:field id="export" link="reports/GradeView.csv?profile_id={parm.profile_id}&amp;bldg_id={parm.bldg_id}&amp;grade_level={parm.grade_level}">Export to .csv</frx:field> 
</frx:fields>
<title>Grade View</title>
</head>
<body>
  <div id="bldg" frx:block="ims/building/building_grade" frx:foreach="*" >
    <h2>{bldg.name}, {grade_name}, {year_label}</h2>
  </div>

  <div id="profile-block" frx:block="ims/building/grade_profile">
    <div frx:foreach="*" id="profile">
      <h3>{name}</h3>
      <div class="pedagoggle-scores" frx:block="ims/building/grade_profile_scores" >
      <table class="table" frx:renderer="FrxPedagoggleScores" frx:profile="profile">
        <thead>
          <tr>
            <th>Name</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <td>{last_name}, {first_name}</td>
          </tr>
        </tbody>
      </table>
      </div>
    </div>
  </div>
  <div id="footer-block" >
    <ul class="nav hnav">
     <li>{export}</li>
    </ul>
  </div>
</body>
</html>