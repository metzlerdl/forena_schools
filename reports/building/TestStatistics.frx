<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE root [
<!ENTITY nbsp "&#160;">
]>
<html xmlns:frx="urn:FrxReports">
<head>
<frx:fields>
	<frx:field id="name"
		link="reports/building.MeasureStatistics?bldg_id={parm.bldg_id}&amp;grade_level={parm.grade_level}&amp;measure_id={measure_id}&amp;school_year={school_year}&amp;seq={seq}">
	</frx:field>
</frx:fields>
<title>Test Statistics</title>
</head>
<body>
	<div class="grid_8">
		<div id="building" frx:block="ims/building/building_grade"
			frx:foreach="*">
			<h2>{building.name}, {grade_name}, {year_label}</h2>
			   <p>The graphs below demonstrate the average normalized according to the following scale:
			   <blockquote>
			     1 - below expectations<br/>
			     2 - struggling<br/>
			     3 - meets expectations<br/>
			     4 - exceeds expectations<br/> 
			   </blockquote>
			   </p>
			   
		</div>
	</div>
	<div class="grid_3 legend">
		<table>
			<tr>
				<th>Building</th>
				<th width="10" style="background-color: tan;"></th>
			</tr>
			<tr>
				<th>District</th>
				<th width="10" style="background-color: olivedrab;"></th>
			</tr>
		</table>
	</div>
	<div class="grid_12">
		<div id="test" frx:block="ims/tests/test" frx:foreach="*">
			<h3>{test.name}</h3>

		</div>
		<div id="schedule" frx:block="ims/building/stat_schedules"
			frx:foreach="*">
			<h4>{sched_label} {year_label}</h4>

			<div id="test_stats" frx:block="ims/building/schedule_stats">
      <svg id="test-chart" frx:renderer="FrxSVGGraph"
        frx:link="building.MeasureStatistics?bldg_id={parm.bldg_id}&amp;grade_level={parm.grade_level}&amp;measure_id={measure_id}&amp;school_year={parm.school_year}"
        frx:type="stackedbargraph" frx:xpath="*" frx:height="600"
        frx:width="800"
        frx:series_1="{bl1_percent}"
        frx:series_2="{bl2_percent}"
        frx:series_3="{bl3_percent}"
        frx:series_4="{bl4_percent}"
        frx:label="{abbrev}"
        frx:colour_1="#E3B5B5"
        frx:colour_2="#E3E288"
        frx:colour_3="#BBE3B5"
        frx:colour_4="#B5C0E3"
        frx:axis_max_v="100.0"
        frx:grid_division_v="10">
      </svg>
				<table>
					<thead>
						<tr>
							<th>measure</th>
							<th>building average</th>
							<th>normalized</th>
							<th>district average</th>
							<th>normalized</th>
							<th>percent met standard</th>
						</tr>
					</thead>
					<tbody>
						<tr frx:foreach="*">
							<td>{name}</td>
							<td>{b_score}</td>
							<td>{b_norm_score}</td>
							<td>{d_score}</td>
							<td>{d_norm_score}</td>
							<td>{b_percent_met}</td>
						</tr>
					</tbody>
				</table>
			</div>
		</div>
	</div>
</body>
</html>