<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE root [
<!ENTITY nbsp "&#160;">
]>
<html xmlns:frx="urn:FrxReports">
<head>
<frx:fields>
  <frx:field id="name" 
  link="reports/building.MeasureStatistics?measure_id={measure_id}&amp;school_year={school_year}&amp;seq={seq}">
  </frx:field>
</frx:fields>
<title>Measure Statistics</title>
</head>
<body>
  <div class="grid_12">
    <div id="test" frx:block="ims/tests/measure" frx:foreach="*">
      <h3>{test_name}: {name}</h3>
    </div>
      <div id="measure_stats" frx:block="ims/building/measure_statistics">
      <svg id="measure-chart"  frx:renderer="FrxSVGGraph"
        frx:type="stackedbargraph" frx:xpath="*" frx:height="{parm.height}" frx:width="{parm.width}"
        frx:options="series[]={bl1_percent}&amp;series[]={bl2_percent}&amp;series[]={bl3_percent}&amp;series[]={bl4_percent}&amp;label={year_label}+{sched_label}&amp;colors[]=#E3B5B5&amp;colors[]=#E3E288&amp;colors[]=#BBE3B5&amp;colors[]=#B5C0E3&amp;axis_max_v=100.0&amp;grid_division_v=10">
    </svg>
        <table>
          <thead>
            <tr><th>measure</th><th>building average</th><th>district average</th><th>struggling</th><th>below</th><th>met</th><th>exceeded</th><th>percent met standard</th></tr>
          </thead>
          <tbody>
            <tr frx:foreach="*">
              <td>{year_label} {sched_label}</td><td>{b_score}</td><td>{d_score}</td><td>{bl1_percent}</td><td>{bl2_percent}</td><td>{bl3_percent}</td><td>{bl4_percent}</td><td>{b_percent_met}</td>
            </tr>
          </tbody>
        </table>
      </div>
  </div>
</body>
</html>