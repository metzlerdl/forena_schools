<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE root [
<!ENTITY nbsp "&#160;">
]>
<html xmlns:frx="urn:FrxReports">
<head>
<frx:fields>
</frx:fields>
<title>Test Statistics</title>
</head>
<body>
	<div class="grid_8">
		<div id="building" frx:block="ims/building/building_grade"
			frx:foreach="*">
			<h2>{building.name} - {grade_name}</h2>
			   <p>The graphs below demonstrate the average normalized according to the following scale:
			   <blockquote>
			     1 - below expectations<br/>
			     2 - struggling<br/>
			     3 - meets expectations<br/>
			     4 - exceeds expectations<br/> 
			   </blockquote>
			   </p>
			   
		</div>
	</div>
	<div class="grid_3 legend">
		<table>
			<tr>
				<th>Building</th>
				<th width="10" style="background-color: tan;"></th>
			</tr>
			<tr>
				<th>District</th>
				<th width="10" style="background-color: olivedrab;"></th>
			</tr>
		</table>
	</div>
	<div class="grid_12">
		<div id="measure" frx:block="ims/tests/measure" frx:foreach="*">
			<h3>{test_name}: {measure.name}</h3>

		</div>
			<div id="strand_stats" frx:block="ims/building/schedule_strand_stats">
			<div
				frx:src="reports/building.ScheduleStrandsGraph.svg?height=400&amp;width=600&amp;measure_id={parm.measure_id}&amp;school_year={parm.school_year}&amp;seq={parm.seq}&amp;bldg_id={parm.bldg_id}&amp;grade_level={parm.grade_level}"
				frx:renderer="FrxInclude" frx:height="420" frx:width="640"></div>
				<table>
					<thead>
						<tr>
							<th>measure</th>
							<th>building average</th>
							<th>district average</th>
							<th>percent met standard</th>
						</tr>
					</thead>
					<tbody>
						<tr frx:foreach="*">
							<td>{name}</td>
							<td>{b_score}</td>
							<td>{d_score}</td>
							<td>{b_percent_met}</td>
						</tr>
					</tbody>
				</table>
			</div>
	</div>
</body>
</html>