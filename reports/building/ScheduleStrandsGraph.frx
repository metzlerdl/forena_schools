<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE root [
<!ENTITY nbsp "&#160;">
]>
<html xmlns:frx="urn:FrxReports">
<head>
<frx:fields>
  <frx:field id="name" 
  link="reports/building.MeasureStatistics?measure_id={measure_id}&amp;school_year={school_year}&amp;seq={seq}">
  </frx:field>
</frx:fields>
<title>Test Statistics</title>
</head>
<body>
  <div class="grid_12" >
    <div id="schedule" frx:block="ims/tests/schedule" frx:foreach="*">
      <h4>{sched_label} {test_name} {year_label}</h4>
  </div>
  <div id="schedule-stats" frx:block="ims/building/schedule_strand_stats">
	  <svg id="test-chart"  frx:renderer="FrxSVGGraph"
	      frx:link="../index.php?q=building/MeasureStatistics&amp;bldg_id={parm.bldg_id}&amp;grade_level={parm.grade_level}&amp;measure_id={measure_id}&amp;school_year={parm.school_year}"
	      frx:type="groupedbargraph" frx:xpath="*" frx:height="{parm.height}" frx:width="{parm.width}"
	      frx:options="series[]={b_norm_score}&amp;series[]={d_norm_score}&amp;label={abbrev}&amp;colors[]=tan&amp;colors[]=olivedrab&amp;axis_max_v=5.0&amp;grid_division_v=1">
	  </svg>
	  <table>
	    <thead>
	      <tr><th>measure</th><th>building average</th><th>district average</th><th>percent met standard</th></tr>
	    </thead>
	    <tbody>
	      <tr frx:foreach="*">
	        <td>{name}</td><td>{b_score}</td><td>{d_score}</td><td>{b_percent_met}</td>
	      </tr>
	    </tbody>
	  </table>
  </div>
  </div>
</body>
</html>