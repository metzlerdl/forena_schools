<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE root [
<!ENTITY nbsp "&#160;">
]>
<html xmlns:frx="urn:FrxReports">
<head>
<frx:fields>
  <frx:field id="name" 
  link="index.php?q=building/MeasureStatistics&amp;measure_id={measure_id}&amp;school_year={school_year}&amp;seq={seq}">
  </frx:field>
</frx:fields>
<title>Measure Statistics</title>
</head>
<body>
  <div class="grid_12">
    <div id="meas" frx:block="ims/tests/measure" frx:foreach="*">
      <h3>{test_name}: {name} {max_value}</h3>
    </div>
      <div id="measure_stats" frx:block="ims/building/measure_statistics">
      <svg id="measure-chart"  frx:renderer="FrxSVGGraph"
        frx:type="groupedbargraph" frx:xpath="*" frx:height="{parm.height}" frx:width="{parm.width}"
        frx:options="series[]={b_score}&amp;series[]={d_score}&amp;label={year_label}+{sched_label}&amp;colors[]=tan&amp;colors[]=olivedrab&amp;axis_max_v={meas.max_value}">
    </svg>
        <table>
          <thead>
            <tr><th>measure</th><th>building average</th><th>district average</th><th>percent met standard</th></tr>
          </thead>
          <tbody>
            <tr frx:foreach="*">
              <td>{year_label} {sched_label}</td><td>{b_score}</td><td>{d_score}</td><td>{b_percent_met}</td>
            </tr>
          </tbody>
        </table>
      </div>
  </div>
</body>
</html>